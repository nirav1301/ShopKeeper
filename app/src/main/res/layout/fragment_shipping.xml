<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ShippingFragment"
    tools:context=".findcustomer.ShippingFragment">

    <TextView
        android:id="@+id/txtShippingnamelabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="64dp"
        android:fontFamily="@font/gilory_light"
        android:text="Name:"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtshippingcompanyname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:fontFamily="@font/gilory_extra_bold"
        android:text="Kyun Kang"
        android:layout_marginStart="10dp"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@id/txtShippingnamelabel"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtShippingStreetLabel"
        android:fontFamily="@font/gilory_light"
        android:text="Street:"
        android:layout_marginStart="60dp"
        android:layout_marginTop="15dp"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/txtShippingnamelabel"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtshippingstreetname"
        app:layout_constraintTop_toBottomOf="@+id/txtshippingcompanyname"
        app:layout_constraintStart_toEndOf="@+id/txtShippingStreetLabel"
        android:layout_marginStart="10dp"
        android:layout_marginTop="15dp"
        android:textSize="18sp"
        android:fontFamily="@font/gilory_extra_bold"
        android:text="2001 S Santafe Ave"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtShippingCityLabel"
        android:layout_marginStart="60dp"
        android:layout_marginTop="15dp"

        app:layout_constraintTop_toBottomOf="@+id/txtShippingStreetLabel"
        app:layout_constraintStart_toStartOf="parent"
        android:text="City:"
        android:textSize="18sp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginStart="10dp"
        android:id="@+id/txtshippingcityname"
        android:text="Los Angeles"
        android:textSize="18sp"
        android:fontFamily="@font/gilory_extra_bold"
        app:layout_constraintTop_toBottomOf="@id/txtshippingstreetname"
        app:layout_constraintStart_toEndOf="@+id/txtShippingCityLabel"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtShippingStateLabel"
        app:layout_constraintTop_toBottomOf="@id/txtShippingCityLabel"
        android:layout_marginStart="60dp"
        android:layout_marginTop="15dp"
        android:text="State/Province:"
        android:textSize="18sp"
        android:fontFamily="@font/gilory_light"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtshippingstatename"
        android:layout_marginTop="15dp"
        android:layout_marginStart="10dp"
        android:text="CA"
        android:fontFamily="@font/gilory_extra_bold"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/txtshippingcityname"
        app:layout_constraintStart_toEndOf="@+id/txtShippingStateLabel"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtShippingCountryLabel"
        android:text="Country:"
        android:textSize="18sp"
        android:fontFamily="@font/gilory_light"
        android:layout_marginTop="15dp"
        android:layout_marginStart="60dp"
        app:layout_constraintTop_toBottomOf="@id/txtShippingStateLabel"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtshippingcountryname"
        android:text="US"
        android:textSize="18sp"
        android:fontFamily="@font/gilory_extra_bold"
        android:layout_marginStart="10dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@id/txtshippingstatename"
        app:layout_constraintStart_toEndOf="@id/txtShippingCountryLabel"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Zip Code:"
        android:id="@+id/txtShippingZipCodeLabel"
        android:textSize="18sp"
        android:fontFamily="@font/gilory_light"
        android:layout_marginStart="60dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@id/txtShippingCountryLabel"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="380008"
        android:textSize="18sp"
        android:fontFamily="@font/gilory_extra_bold"
        android:id="@+id/txtshippingzipcode"
        android:layout_marginTop="15dp"
        android:layout_marginStart="10dp"
        app:layout_constraintTop_toBottomOf="@+id/txtshippingcountryname"
        app:layout_constraintStart_toEndOf="@+id/txtShippingZipCodeLabel"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone:"
        android:id="@+id/txtShippingPhoneLabel"
        android:textSize="18sp"
        android:fontFamily="@font/gilory_light"
        android:layout_marginStart="60dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/txtShippingZipCodeLabel"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7990427423"
        android:textSize="18sp"
        android:layout_marginTop="15dp"
        android:layout_marginStart="10dp"
        android:id="@+id/txtshippingphone"
        android:fontFamily="@font/gilory_extra_bold"
        app:layout_constraintTop_toBottomOf="@id/txtshippingzipcode"
        app:layout_constraintStart_toEndOf="@id/txtShippingPhoneLabel"/>

    <ImageView
        android:id="@+id/imgclosefragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:padding="10dp"
        android:layout_marginEnd="36dp"
        android:src="@drawable/ic_close1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>